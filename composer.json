{
    "name": "zenaton/zenaton-php",
    "type": "library",
    "description": "Zenaton PHP library",
    "keywords": [
        "workflow engine",
        "workflows",
        "orchestration",
        "event-driven architecture",
        "queuing systems",
        "orchestration engine",
        "background jobs",
        "hosted queues",
        "queues",
        "jobs",
        "asynchronous tasks"
    ],
    "homepage": "https://zenaton.com",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
        "cakephp/chronos": "^1.1",
        "jeremeamia/superclosure": "^2.2",
        "nategood/httpful": "^0.2",
        "ramsey/uuid": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.27"
    },
    "config": {
        "platform": {
            "php": "5.6.6"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Zenaton\\Exceptions\\": "src/Zenaton/Exceptions/",
            "Zenaton\\": "src/Zenaton/v1/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zenaton\\Test\\": "tests/Zenaton/Test/",
            "Zenaton\\": "tests/Zenaton/v1/"
        },
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
        ]
    }
}
